{
  "name": "nestjs-grammy",
  "version": "0.0.1",
  "description": "A Grammy library providing nestjs integration",
  "author": "Daniil Jave",
  "license": "MIT",
  "main": "dist/index.js",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "prepare": "husky install && sed -i 's@#!/bin/sh@#!/usr/bin/env bash@g' .husky/_/husky.sh && sed -i 's@sh -e@bash -e@g' .husky/_/husky.sh",
    "typedoc": "typedoc"
  },
  "contributors": [
    {
      "name": "Daniil Jave",
      "email": "javeoff@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/javeoff/nestjs-grammy"
  },
  "keywords": [
    "grammy",
    "nestjs",
    "telegram",
    "bot",
    "telegram bot",
    "nestjs bot",
    "nest bot"
  ],
  "dependencies": {
    "@nestjs/common": "^9.1.2",
    "@nestjs/core": "^9.1.2",
    "grammy": "^1.11.1",
    "reflect-metadata": "^0.1.13",
    "typescript": "^4.8.3"
  },
  "peerDependencies": {
    "@nestjs/common": "^8.0.0 || ^9.0.0",
    "@nestjs/core": "^8.0.0 || ^9.0.0",
    "reflect-metadata": "^0.1.13",
    "grammy": "^1.11.1",
    "typescript": "^4.1.2"
  }
}
